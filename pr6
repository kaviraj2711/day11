#include <stdio.h>

int main()
{
    char s[100];
    scanf("%[^\n]",s);
    int i, v=0,c=0,sp=0,d=0;
    for(i=0 ; s[i] != '0' ; i++)
    {
        if(s[i] == 'a' || s[i] == 'e' || s[i] == 'i' || s[i] == 'o' || s[i] == 'u')
        v++;
        else if(s[i]==' ')
        sp++;
        else if(s[i] >= '0' && s[i] <= '9')
        d++;
        else
        c++;
    }
    printf("vowels=%d\n space=%d\n consonants=%d\n digit=%d\n",v,sp,c,d);

    return 0;
}
